                    </valuemap>
                    <triggers>
                        <trigger>
                            <uuid>68a6f88e983d4b4abb48f9310a74a6bb</uuid>
                            <expression>last(/Brother/toner.level.raw)&lt;5</expression>
                            <name>Toner baixo - Verificar necessidade de reposição</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <tags>
